{
  "manifest_version": 2,

  "name": "Tab Menu",
  "description": "Select, close, rearrange, and search your tabs from a toolbar menu!",
  "version": "7.1.4",

  "browser_action": {
    "default_icon": "35.png",
    "default_popup": "menu.html"
  },

  "icons": {
    "16": "16.png",
    "32": "35.png",
    "48": "48.png",
    "128": "128.png"
  },

  "permissions": [ "tabs" ],

  "update_url": "https://clients2.google.com/service/update2/crx"
}
